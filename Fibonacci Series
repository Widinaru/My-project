#include <bits/stdc++.h>

using namespace std;

int main() {
	int fib[100];
	int n;
	cout << "Enter number: ";
	cin >> n;
	fib[0] = 1;
	fib[1] = 1;
	for(int i=2;i<n;i++) {
		fib[i] = fib[i-1] + fib[i-2];
	}
	cout << "\nFibonacci series: ";
	for(int i=0;i<n;i++) {
		cout<<fib[i]<<" ";
	}
	cout << endl;
	system ( "Pause" );
	return 0;
}
